    background-color: #aaa;
}
QListWidget::item:disabled {


}
QListWidget::item::pressed  {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #000000, stop: 1 #111111);
}
