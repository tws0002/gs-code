QScrollBar:vertical {
    border: 0px solid grey; 
    border-radius: 4px;
    background: #484848;
    width: 8px; 
    margin: 0px 0px 0px 0px; 
}
QScrollBar:horizontal {
    border: 0px solid grey; 
    border-radius: 4px;
    background: #484848;
    height: 8px; 
    margin: 0px 0px 0px 0px; 
}
QScrollBar:handle {
    border: 0px solid grey; 
    border-radius: 4px;
    background: #97a0a5;
    width: 10px; 
    margin: 0px 0px 0 0px;
}
QScrollBar::add-line {
    border-radius: 10px;
}
QScrollBar::sub-line {
    border-radius: 10px;
}
QScrollBar::sub-page {
    background: #484848;
    border-radius: 10px;
}

QScrollBar::add-page {
    background: #484848;
    border-radius: 10px;
}

