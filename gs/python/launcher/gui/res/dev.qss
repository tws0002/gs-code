QPushButton {
    background-color: #333333;
    font: bold 10pt;
    font-style:bold;
    text-align: center;
    selection-background-color: #ebebeb;
    border: none; /* no border for a flat push button */
    border-radius: 2px;
    padding: 8px 8px 8px 8px;
}
QPushButton:hover {
   background-color: #444444;
}
QPushButton:pressed  {
   background-color: #383838;
}

