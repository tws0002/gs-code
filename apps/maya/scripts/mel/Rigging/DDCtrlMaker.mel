global proc string wireShapeScale(string $type, float $scale )
{
	string $curve = `wireShape $type`;
	string $selected[] = `ls -sl`;
	select ($curve+".cv[*]");
	scale -r $scale $scale $scale ;
	if(`size $selected`){ select $selected; }
	return ( $curve );
}