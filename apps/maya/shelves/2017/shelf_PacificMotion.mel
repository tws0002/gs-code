        -command "//Primary Underslung Button\n\nstring $whichrig = `getAttr KuperMachine.RigType`;\n\nif ($whichrig == \"GazellePTR\")\n{\n\t//Underslung Gazelle PTR\n\tsetAttr \"SorensonRotator_Pan.rotateX\" 0;\n\tsetAttr \"SorensonRotator_Pan_pointConstraint1.offsetY\" 18.268;\n\tsetAttr \"CenterOfRotation_pointConstraint1.offsetY\" 0;\n\tsetAttr \"KuperMachine.PosOrNeg\" -1;\n\ttransformLimits -ty -48.296069 43.20394 -ety 1 1 AnimateMe;\n\tprint \"Gazelle Pan Tilt Roll is now in Underslung mode\";\n}\n\nelse if ($whichrig == \"GazellePT\")\n{\n//Underslung Gazelle Pan and Tilt\nsetAttr \"SorensonRotator_Pan.rotateX\" 0;\nsetAttr \"SorensonRotator_Pan_pointConstraint1.offsetY\" 20.625;\n\nsetAttr \"KuperMachine.PosOrNeg\" -1;\nsetAttr \"DB_Hook.translateY\" 0;\ntransformLimits -ty -48.296069 43.20394 -ety 1 1 AnimateMe;\nprint \"Gazelle Pan Tilt is now in Underslung mode\";\n}\n\nelse if ($whichrig == \"GraphlitePTR\")\n{\n//Underslung ForGraphlite PTR\nsetAttr \"SorensonRotator_Pan.rotateX\" 0;\nsetAttr \"SorensonRotator_Pan_pointConstraint1.offsetY\" 18.268;\n\nsetAttr \"KuperMachine.PosOrNeg\" -1;\n\nsetAttr \"CenterOfRotation_pointConstraint1.offsetY\" 0;\ntransformLimits -ty -44.561624 89.438376 -ety 1 1 AnimateMe;\nprint \"Graphlite Pan Tilt Roll is now in Underslung mode\";\n}\n\nelse if ($whichrig == \"GraphlitePT\")\n{\n//Under ForGraphlite PT\nsetAttr \"SorensonRotator_Pan.rotateX\" 0;\nsetAttr \"SorensonRotator_Pan_pointConstraint1.offsetY\" 20.625;\nsetAttr \"CenterOfRotation_pointConstraint1.offsetY\" 0;\n//53.561624 = Home Height\n//For Graphlite Underslung 143 is max height and min is -10 inches but im making it +9\ntransformLimits -ty -44.561624 89.438376 -ety 1 1 AnimateMe;\nprint \"Graphlite Pan Tilt is now in Underslung mode\";\n}\n\nelse if ($whichrig == \"ZebraPTR\")\n{\n//Zebra Pan Tilt Roll\n//Underslung For Zebra PTR\nsetAttr \"SorensonRotator_Pan.rotateX\" 0;\nsetAttr \"DivingBoardHook.translateY\" 0;\nsetAttr \"AnimateMe_pointConstraint1.offsetY\" -18.268;\n\nsetAttr \"Castelnut.translateY\" 0;\nsetAttr \"Castelnut.rotateZ\" 0;\nprint \"Zebra Pan Tilt Roll is now in Underslung mode\";\n}\n\nelse if ($whichrig == \"ZebraPT\")\n{\n//Zebra Pan Tilt\n//Underslung For Zebra PT\nsetAttr \"SorensonRotator_Pan.rotateX\" 0;\nsetAttr \"DivingBoardHook.translateY\" 0;\nsetAttr \"AnimateMe_pointConstraint1.offsetY\" -20.625;\n\nsetAttr \"Castelnut.translateY\" 0;\nsetAttr \"Castelnut.rotateZ\" 0;\nprint \"Zebra Pan Tilt is now in Underslung mode\";\n}\n\n\n" 
